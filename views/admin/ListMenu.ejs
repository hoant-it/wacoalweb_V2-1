<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../block/head %>
        <link rel="stylesheet" href="/css/dataTables.bootstrap4.min.css">
        <link rel="stylesheet" href="/css/jquery.dataTables.min.css">
        <link rel="stylesheet" href="/css/admin/userList.css">
</head>

<body class="nav-md footer_fixed">

    <div class="container body">
        <div class="main_container">
            <% include ../block/leftpanelV2 %>
                <% include ../block/toppanelV2 %>
                    <!-- <form action="/userListv2" method="POST">     -->
                    <form role="form" id="frmSaveData">
                        <!-- page content -->
                        <div class="right_col " role="main" style="min-height: 100vh">
                            <div class="page-title">
                                <div class="title_left">
                                    <h3> <small><strong></strong></small></h3>
                                </div>
                            </div>
                            <div class="clearfix"></div>

                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                    <div class="x_panel">
                                        <div class="x_title">
                                            <h2>Menu List</h2>
                                            <ul class="nav navbar-right panel_toolbox">
                                                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a> </li>
                                            </ul>
                                            <div class="clearfix"></div>
                                        </div>

                                        <div class="x_content" id="xcontentid">

                                            <table class="table table-striped table-bordered mydatatable" style="width: 100%;">
                                                <thead>
                                                    <tr>
                                                        <th>MenuCode </th>
                                                        <th>FormName</th>
                                                        <th>FormCode</th>
                                                        <th>ProjectCode</th>
                                                        <th>FolderContainForm</th>
                                                        <th>MenuImage</th>
                                                        <th>GuideDocument</th>
                                                        <th>ProcessDocument</th>
                                                        <th>ShowCode</th>
                                                        <th>IsActive</th>
                                                        <th>WorkFunCode</th>
                                                        <th>SystemName</th>
                                                        <th>Href</th>
                                                    </tr>

                                                </thead>
                                                <tbody>
                                                    <% if ( arrListMenu) { %>
                                                        <% for( var i=0 ; i < arrListMenu.length ; i++) { %>
                                                            <tr>
                                                                <td>
                                                                    <%=arrListMenu[i].MenuCode %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].FormName %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].FormCode %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].ProjectCode %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].FolderContainForm %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].MenuImage %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].GuideDocument %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].ProcessDocument %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].ShowCode %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].IsActive %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].WorkFunCode %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].SystemName %>
                                                                </td>
                                                                <td>
                                                                    <%=arrListMenu[i].Href %>
                                                                </td>
                                                            </tr>
                                                            <% } %>
                                                                <% } else { %>
                                                                    <tr>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                    </tr>
                                                                    <% } %>
                                                </tbody>

                                            </table>

                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!-- Modal -->
                            <div class="modal fade" id="modalAddUpdate" role="dialog">
                                <div class="modal-dialog">

                                    <!-- Modal content-->
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            <h4 class="modal-title">List Menu Update</h4>
                                        </div>
                                        <div class="modal-body">

                                            <div class="form-group">
                                                <label for="txtMenuCode">Menu Code</label>
                                                <input type="text" class="form-control" name="txtMenuCode" id="txtMenuCode" value="<%=Menucode %>">
                                                <!-- <input type="hidden" id="hidID" value="0" /> -->
                                            </div>
                                            <div class="form-group">
                                                <label for="txtFormName">Form Name:</label>
                                                <input type="text" class="form-control" name="txtFormName" id="txtFormName">
                                            </div>
                                            <div class="form-group">
                                                <label for="txtFormCode"> Form Code:</label>
                                                <input type="text" class="form-control" name="txtFormCode" id="txtFormCode">
                                            </div>
                                            <div class="form-group">
                                                <label for="txtprojectCode"> Project Code:</label>
                                                <input type="text" class="form-control" name="txtprojectCode" id="txtprojectCode">
                                            </div>
                                            <div class="form-group">
                                                <label for="txtshowCode"> Show Code:</label>
                                                <input type="text" class="form-control" name="txtshowCode" id="txtshowCode">
                                            </div>
                                       
                                            <div class="form-group">
                                                <label for="txtSystemName"> system Name:</label>
                                                <input type="text" class="form-control" name='txtSystemName' id="txtSystemName">
                                            </div>
                                            <div class="modal-footer">
                                                <button type="submit" name="submitInsert" value="submitInsert" class="btn btn-primary" id="btnSave">Save</button>
                                                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                            </div>
                                            <!-- <div class="checkbox">
                                            <label><input type="checkbox" id="ckStatus">Active</label>
                                            </div> -->

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /page content -->

                        <!-- footer content -->
                        <footer>
                            <div class="nav_menu">
                                <nav class="nav navbar-nav">
                                    <ul class=" navbar-right">
                                        <li style="padding-right: 15px; float: right; margin-top: 20px;">
                                            <button type="button" class="btn btn-success" id="btnAddNew">New</button>
                                            <button type="button" class="btn btn-info" id="btnedit">Edit</button>
                                            <button type="submit" name="bsubmit" value="submitDelete" class="btn btn-danger" id="btnDeleteId">Delete</button>
                                            <!-- <button type="submit" name="bsubmit" value="submitSave" class="btn btn-primary" id="btnSaveId">Save</button>
                                        <button type="button" onclick="cancelUser()" name="bsubmit" value="submitCancel" class="btn btn-secondary" id="btnCancelId">Cancel</button> -->
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </footer>
                        <!-- /footer content -->
        </div>
    </div>
    <% include ../block/script %>
    <script>
        //lay gia tri bien truyen vao tu router
        var test = '<%- Menucode %>';
        // console.log('test '+test ) // You have to use var to make it global
    </script>
        <!-- <script src="/js/popper.min.js"></script>
        <script src="/js/bootstrap.min.js"></script> -->
        <script src="/js/jquery.dataTables.min.js"></script>
        <script src="/javascripts/admin/ListMenujs.js"></script>
        <!-- </form> -->
        </form>
</body>

</html>