                                                        <th>CNY010</th>  
                                                        <th>CompanyNameAlt</th>  
                                                        <th>AddressAlt</th>  
                                                        <th>CNY011</th> 
                                                        <th>CNY012</th>
                                                    </tr>

                                                </thead>
                                                <tbody>
                                                    <% if ( arrListCompany) { %>
                                                        <% for( var i=0 ; i < arrListCompany.length ; i++) { %>
                                                            <tr>
                                                                <td><%=arrListCompany[i].CompanyCode %></td>
                                                                <td><%=arrListCompany[i].CompanyName %></td>
                                                                <td><%=arrListCompany[i].Adrress %></td>
                                                                <td><%=arrListCompany[i].PhoneNumber %></td>
