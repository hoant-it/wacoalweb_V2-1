                                                                <td><%=arrListCompany[i].MaSoThue %></td>
                                                                <td><%=arrListCompany[i].Tax %></td>
                                                                <td><%=arrListCompany[i].CNY006 %></td>
                                                                <td><%=arrListCompany[i].CNY007 %></td>
                                                                <td><%=arrListCompany[i].Email %></td>
                                                                <td><%=arrListCompany[i].CNY009 %></td>
                                                                <td><%=arrListCompany[i].CNY010 %></td>
                                                                <td><%=arrListCompany[i].CompanyNameAlt %></td>
                                                                <td><%=arrListCompany[i].AddressAlt %></td>
                                                                <td><%=arrListCompany[i].CNY011 %></td>
                                                                <td><%=arrListCompany[i].CNY012 %></td>
                                                      
