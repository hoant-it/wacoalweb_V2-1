<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../block/head %>
        <!-- <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"> -->
        <link rel="stylesheet" href="/css/dataTables.bootstrap4.min.css">
        <link rel="stylesheet" href="/css/jquery.dataTables.min.css">
        <!-- <link rel="stylesheet" href="/css/admin/userList.css"> -->
</head>

<body class="nav-md footer_fixed">

    <div class="container body">
        <div class="main_container">
            <% include ../block/leftpanelV2 %>
                <% include ../block/toppanelV2 %>
                    <form id="my-form" action="/kho/productsize" method="POST">
                        <!-- page content -->
                        <div class="right_col " role="main" style="min-height: 100vh">
                            <div class="page-title">
                                <div class="title_left">
                                    <h3> <small><strong></strong></small></h3>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                    <div class="x_panel">
                                        <div class="x_title">
                                            <h2>Size/Cup SP <small></small></h2>
                                            <ul class="nav navbar-right panel_toolbox">
                                                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                                </li>
                                            </ul>
                                            <div class="clearfix"></div>
                                        </div>
                                        <div class="x_content">
                                            <br>

                                            <div class="form-label-left input_mask">
                                                <label class="col-form-label col-md-1 col-sm-1 label-align" for="first-name">Code <span class="required">*</span>
                                                </label>
                                                <div class="col-md-3 col-sm-3 ">
                                                    <input type="text" id="codeID" name="code" required="required" class="form-control ">
                                                </div>
                                                <label class="col-form-label col-md-1 col-sm-1 label-align" for="first-name">Name (VN)<span class="required"></span>
                                                </label>
                                                <div class="col-md-3 col-sm-3 ">
                                                    <input type="text" id="nameVNId" name="namevn" class="form-control ">
                                                </div>
                                                <label class="col-form-label col-md-1 col-sm-1 label-align" for="first-name">Name (EN)<span class="required"></span>
                                                </label>
                                                <div class="col-md-3 col-sm-3 ">
                                                    <input type="text" id="nameENID" name="nameen" class="form-control ">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                    <div class="x_panel">
                                        <div class="x_title">
                                            <h2>DS Size/Cup</h2>
                                            <ul class="nav navbar-right panel_toolbox">
                                                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a> </li>
                                            </ul>
                                            <div class="clearfix"></div>
                                        </div>

                                        <div class="x_content" id="xcontentid">

                                            <table class="table table-striped table-bordered mydatatable" style="width: 100%;">
                                                <thead>
                                                    <tr>
                                                        <th>Code</th>
                                                        <th>Name (VN)</th>
                                                        <th>Name (EN)</th>
                                                    </tr>

                                                </thead>
                                                <tbody>
                                                    <% if ( arrProductSize) { %>
                                                        <% for( var i=0 ; i < arrProductSize.length ; i++) { %>
                                                            <tr>
                                                                <td>
                                                                    <%=arrProductSize[i].SIZECODE %>
                                                                </td>
                                                                <td>
                                                                    <%=arrProductSize[i].SIZENAME_VN %>
                                                                </td>
                                                                <td>
                                                                    <%=arrProductSize[i].SIZENAME_EN %>
                                                                </td>
                                                            </tr>
                                                            <% } %>
                                                                <% } else { %>
                                                                    <tr>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                        <td> Empty </td>
                                                                    </tr>
                                                                    <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <!-- /page content -->

                        <!-- footer content -->
                        <footer>
                            <div class="nav_menu">
                                <nav class="nav navbar-nav">
                                    <ul class=" navbar-right">
                                        <li style="padding-right: 15px; float: right; margin-top: 20px;">
                                            <button type="button" onclick="New()" class="btn btn-success" id="btnNewId">New</button>
                                            <button type="submit" name="bsubmit" value="submitDelete" class="btn btn-danger" id="btnDeleteId">Delete</button>
                                            <button type="submit" name="bsubmit" value="submitSave" class="btn btn-primary" id="btnSaveId">Save</button>
                                            <button type="button" onclick="Cancel()" name="bsubmit" value="submitCancel" class="btn btn-secondary" id="btnCancelId">Cancel</button>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </footer>
                        <!-- /footer content -->
                    </form>

        </div>
    </div>
    <% include ../block/script %>
        <script src="/js/jquery.dataTables.min.js"></script>
        <script src="/js/dataTables.bootstrap4.min.js"></script>
        <script src="/javascripts/kho/KhoProoductsize.js"></script>
        <script>
            var test = '<%- mess %>';
            if (test !== '') {
                alert(test);
            }
        </script>
</body>

</html>